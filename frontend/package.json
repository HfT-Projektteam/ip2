{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^5.4.6",
    "dayjs": "^1.11.7",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "serve": "^14.2.0",
    "styled-components": "^5.3.10",
    "typescript": "*",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_DEPLOY_ENV=prd craco start",
    "start:dev": "set REACT_APP_DEPLOY_ENV=dev&& craco start",
    "build": "REACT_APP_DEPLOY_ENV=prd craco build",
    "build:dev": "set REACT_APP_DEPLOY_ENV=dev&& craco build",
    "test": "craco test --passWithNoTests --coverage",
    "test:watch": "craco test --watch --passWithNoTests --coverage",
    "eject": "craco eject",
    "lint": "eslint --quiet **/src/**/*.{ts,tsx}",
    "lint:fix": "eslint . --fix **/src/**/*.{ts,tsx}",
    "format": "prettier --write **/src/**/*.{ts,tsx,css,md,json} --config ./.prettierrc",
    "format:check": "prettier --check **/src/**/*.{ts,tsx,css,md,json} --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "circular-dependency-plugin": "^5.2.2",
    "craco-alias": "^3.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "history": "^5.3.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "dotenv": "^16.2.0",
    "react-infinite-scroll-component": "^6.1.0"
  },
  "cracoConfig": "craco.config.js",
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/*",
      "app.tsx",
      "index.tsx",
      "reportWebVitals.ts",
      "/assets/*",
      "/components/layout/OldHeader/*",
      "/data/*",
      "/services/BackendAPI/*",
      "/services/IdGenerator/*",
      "/services/SpotifyAPI/component.ts"
    ],
    "coverageReporters": ["json-summary"]

  }
}
